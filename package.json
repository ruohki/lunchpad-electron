{
  "name": "lunchpad",
  "productName": "Lunchpad",
  "version": "0.0.1",
  "description": "Lunchpad is a Desktop Application to leverage your Launchpad",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "rebuild": "electron-rebuild",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [],
  "author": {
    "name": "ruohki",
    "email": "ruohki@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "e:/Lunchpad/images/LP_Icon.png"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "lunchpad"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/index.js",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@electron-forge/cli": "^6.0.0-beta.47",
    "@electron-forge/maker-deb": "^6.0.0-beta.47",
    "@electron-forge/maker-rpm": "^6.0.0-beta.47",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.47",
    "@electron-forge/maker-zip": "^6.0.0-beta.47",
    "@electron-forge/plugin-auto-unpack-natives": "^6.0.0-beta.47",
    "@electron-forge/plugin-webpack": "^6.0.0-beta.47",
    "@fortawesome/fontawesome-pro": "^5.12.0",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-wildcard": "^5.0.0",
    "babel-polyfill": "^6.26.0",
    "css-loader": "^3.4.0",
    "electron": "^7.1.7",
    "file-loader": "^5.0.2",
    "fork-ts-checker-webpack-plugin": "^3.1.1",
    "node-loader": "^0.6.0",
    "style-loader": "^0.23.1",
    "svg-url-loader": "^3.0.3",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.4",
    "url-loader": "^3.0.0"
  },
  "dependencies": {
    "electron-settings": "^3.2.0",
    "electron-squirrel-startup": "^1.0.0",
    "howler": "^2.1.3",
    "lodash": "^4.17.15",
    "midi": "^1.0.0",
    "number-pairings": "^1.7.5",
    "polished": "^3.4.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-dropdown": "^1.6.4",
    "react-tippy": "^1.3.1",
    "react-use": "^13.13.0",
    "robotjs": "git+https://github.com/ruohki/robotjs.git",
    "styled-components": "^4.4.1",
    "typeface-exo-2": "0.0.72",
    "uuid": "^3.3.3"
  }
}
